UPDATE DEV_RAW.Dynamics_CRM.CRM_Appointments
SET SCHEDULEDSTART = 
    DATEADD(
        hour, DATE_PART(hour, SCHEDULEDSTART), 
        DATEADD(
            minute, DATE_PART(minute, SCHEDULEDSTART), 
            DATEADD(
                second, DATE_PART(second, SCHEDULEDSTART), 
                DATE_FROM_PARTS(
                    YEAR(SCHEDULEDSTART) - 100, 
                    MONTH(SCHEDULEDSTART), 
                    DAY(SCHEDULEDSTART)
                )
            )
        )
    )
WHERE YEAR(SCHEDULEDSTART) BETWEEN 2095 AND 2099;
